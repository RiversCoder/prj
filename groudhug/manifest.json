{
  "name": "FreeCtrl",
  "version": "2.0",
  "manifest_version": 2,
  "description": "FreeCtrl",
  "content_scripts": [
    { 
      "matches": ["http://*/*"],
      "js": ["./pages/js/pageActor.js"]   
    }
  ],
  "browser_action": {
    "default_icon": "./images/icon1.png",
    "default_popup": "./pages/popup.html",
    "name": "Click To Active FreeCtrl"
  },
  "options_page": "./pages/options.html",
  "permissions": [
    "tabs", "http://*/*"
  ]
}
